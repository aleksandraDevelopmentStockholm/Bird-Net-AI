{
  "name": "aiapp",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "pnpm expo start",
    "start:dev": "cp .env.dev .env && expo start --clear",
    "start:prod": "cp .env.prod .env && expo start --clear",
    "test-server": "node ./test-server/mock-api.js",
    "android": "pnpm expo run:android",
    "android:dev": "cp .env.dev .env && pnpm expo run:android",
    "android:prod": "cp .env.prod .env && pnpm expo run:android",
    "ios": "pnpm expo run:ios",
    "ios:dev": "cp .env.dev .env && pnpm expo run:ios",
    "ios:prod": "cp .env.prod .env && pnpm expo run:ios",
    "ios:release": "pnpm expo run:ios --configuration Release",
    "web": "pnpm expo start --web",
    "lint": "pnpm expo lint",
    "lint:fix": "pnpm expo lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "fetch-bird-urls": "ts-node infra/scripts/fetch-bird-image-urls.ts",
    "test:e2e": "maestro test .maestro/",
    "test:e2e:ci": "maestro test .maestro/home.yaml .maestro/navigation.yaml .maestro/recording-flow-mock.yaml --format junit",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "version": "echo \"Current version: $(node -p \"require('./package.json').version\")\"",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "build:increment": "node scripts/increment-build.js",
    "build:increment:ios": "node scripts/increment-build.js ios",
    "build:increment:android": "node scripts/increment-build.js android"
  },
  "dependencies": {
    "@babel/runtime": "^7.28.4",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/slider": "^5.0.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "dotenv": "^17.2.3",
    "expo": "~54.0.21",
    "expo-asset": "~12.0.9",
    "expo-audio": "^1.0.14",
    "expo-constants": "~18.0.10",
    "expo-file-system": "^19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.14",
    "expo-sensors": "^15.0.7",
    "expo-splash-screen": "~31.0.9",
    "expo-symbols": "~1.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@types/node": "^24.8.1",
    "@types/react": "~19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cross-env": "^10.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "expo-dev-client": "~6.0.16",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "pnpm": {
    "overrides": {
      "tar@7.5.1": ">=7.5.2"
    }
  },
  "private": true
}
