# BirdNet App Environment Variables
# Copy this file to .env and fill in your actual values

# App Configuration
APP_NAME=your-app-name-here
APP_SLUG=your-app-slug-here
IOS_BUNDLE_ID=your.ios.bundle.id.here
ANDROID_PACKAGE=your.android.package.here

# EAS Project
EAS_PROJECT_ID=your-eas-project-id-here

# BirdNet API Configuration
# Get your API key from your BirdNet API provider
EXPO_PUBLIC_BIRDNET_API_URL=https://your-api-gateway-url.amazonaws.com/analyze
EXPO_PUBLIC_BIRDNET_API_KEY=your-api-key-here

# Mock Mode (for testing without real API)
# Set to 'true' to use mock server, 'false' for production
EXPO_PUBLIC_MOCK_MODE=false
EXPO_PUBLIC_MOCK_SCENARIO=success

# Apple App Store Connect (for EAS Submit)
# Note: These should be set as EAS secrets, not in .env
# Run: eas secret:create --scope project --name APPLE_ID --value "your-email@example.com"
# APPLE_ID=your-apple-id@example.com
# ASC_APP_ID=1234567890
# APPLE_TEAM_ID=ABCD123456
